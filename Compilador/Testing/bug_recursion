                                                /% Group 2 %/

                          /% ------------- Program without errors.  ------------------- %/


 LONGINT x;
 x = 8_l;
 OUT("INICIO");
 PROC procedure_1 (LONGINT p4) NA = 1_l , SHADOWING = TRUE
 {
    OUT("ENTRO EN PROC");
    x = x + 1_l;
    IF(x > 11_l){
        OUT("MULTIPLICA BIEN PERRITO SUCIO");
    }ELSE{
        procedure_1(x);
    }
    END_IF;

 };
 OUT("INVOCO A PROC RECURSIVO");
 procedure_1(x);
 OUT("FIN DE PROGRAMA");

